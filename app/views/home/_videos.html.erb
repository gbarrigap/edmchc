<section class="galeria videos hero is-fullheight-with-navbar" id="videos">
  <div class="hero-body">
    <div class="container is-fluid has-text-centered">
      <h1 class="title is-1">Videos
        <%= @encuentro.version %>
      </h1>
      <% if @encuentro.hay_videos? %>
        <% @encuentro.videos.lines.each do |l| %>
        <iframe width="560" height="315" src="<%= l %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        <% end %>
      <% end %>
    </div>
  </div>
</section>
